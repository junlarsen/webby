{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "docker:dev": {},
    "docker:push": {
      "dependsOn": ["^docker:dev"]
    }
  }
}
